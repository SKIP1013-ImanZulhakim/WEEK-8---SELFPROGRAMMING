package selfProgramming;
import java.util.Scanner;
public class SumTen {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num=0, sum=0;
		
		System.out.println("Please enter 10 numbers");
		
		for(int i=0; i<10; i++) {
			num=sc.nextInt();
			sum+=num;
		}
		System.out.println("The sum is " + sum);
	}

}
